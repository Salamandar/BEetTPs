SBendArc2.SetAttr "RadiusType", 0 
SBendArc2.SetAttr "NewPosBase", 0 
SBendArc2.SetAttr "Depth", "0" 
SBendArc2.SetAttr "StartThickness", "0.000000" 
SBendArc2.SetAttr "EndThickness", "0.000000" 
SBendArc2.SetProfileName "guidecanal1" 
SBendArc2.SetDefaultThicknessTaperMode True 




