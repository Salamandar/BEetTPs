% k=0.49;
% m=0.126;
% i=1;
% 
% while((m-floor(m)>10e-4)|(k-floor(k)>10e-4))
%     k=0.49*i;
%     m=0.126*i;
%     i=i+1;
% end
%        
%  i-1  

clear all
close all
clc
format long

lambda1=1.55;
k1=2*pi/lambda1;
neff1=1.5020053;

lambda2=1.31;
k2=2*pi/lambda2;
neff2=1.5032;

nc=1.51;
ns=1.5;
a2=3;
a=a2/2;
d=2;

alpha1 = k1*sqrt(nc^2-neff1^2);
alpha2 = k2*sqrt(nc^2-neff2^2);

kappa1 = k1*sqrt(neff1^2-ns^2);
kappa2 = k2*sqrt(neff2^2-ns^2);

V1=k1*a2*sqrt(nc^2-ns^2);
V2=k2*a2*sqrt(nc^2-ns^2);

u1=alpha1*a;
u2=alpha2*a;

v1=kappa1*a;
v2=kappa2*a;

C1=v1^2*u1^2*exp(-v1*d/a)/(k1*neff1*(1+v1)*a^2*V1^2)
C2=v2^2*u2^2*exp(-v2*d/a)/(k2*neff2*(1+v2)*a^2*V2^2)


L1=pi/(4*C1)
L2=pi/(4*C2)


z=[0:0.001:20];

plot(z,sin(z*L1),z,sin(z*L2))

















































































































































































































































































































































































































































































































































































